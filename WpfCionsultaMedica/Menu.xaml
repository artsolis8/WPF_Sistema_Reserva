<Window x:Class="WpfCionsultaMedica.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCionsultaMedica"
        mc:Ignorable="d"
        Title="Menu" WindowState="Maximized" Background="Black">
    <Menu Name="MenuPrincipal">
        <MenuItem Header="Opciones" Click="MenuItem_Click_1">
            <MenuItem Header="Consultorios" Click="MenuItem_Click" />
            <MenuItem Header="Turno" Click="MenuItem_Click_2" />
            <MenuItem Header="Clinica" Click="MenuItem_Click_3"/>
            <Separator />
            <MenuItem Header="Salir" />
        </MenuItem>
        <MenuItem Header="Procesos">
            <MenuItem Header="Registro Citas" Click="Citas_Click_" />
            <MenuItem Header="Api Pacientes" Click="MenuItem_Click_4"/>
            <Separator />
        </MenuItem>

    </Menu>
</Window>
